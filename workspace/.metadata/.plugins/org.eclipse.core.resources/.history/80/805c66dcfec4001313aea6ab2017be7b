#include "FreeRTOS/queue.h"
#include "timer.h"
#include "panic.h"

static xQueueHandle timer_queue;

void init_timer()
{
	timer_queue = xQueueCreate(1, sizeof(int));
	if (timer_queue == NULL) {
		panic("init_timer: unable to init timer queue");
	}
}
